{{!-- reset-password --}}
  <legend class="uk-legend uk-text-center">Ingrese una nueva contraseÃ±a</legend>
  <legend class="uk-legend uk-text-center">Recuperar cuenta</legend>
<form
  action="/users/reset-password/{{token}}"
  method="post"
  style=""
  class="formReset"
>




  <div class="uk-margin">
    <div class="uk-inline">
      <a class="uk-form-icon uk-form-icon-flip cursor-pointer" uk-icon="icon: lock"></a>
      <input
      autofocus
        class="uk-input"
        type="password"
        aria-label="Not clickable icon"
        placeholder="********"
        name="password"
        required
      />
    </div>
  </div>

  <button
    class="uk-button uk-button-primary uk-width-1-1 uk-margin-small-bottom"
    type="submit"
  >Enviar</button>

  <hr align="center" />
</form>
{{!-- reset-password --}}

<script type="module" src="/js/reset.js"></script>